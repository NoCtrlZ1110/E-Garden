    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>UET.EGarden</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Abp.EntityFrameworkCore.EFPlus" Version="5.6.0" />
    <PackageReference Include="NPOI" Version="2.4.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\UET.EGarden.Application.Shared\UET.EGarden.Application.Shared.csproj" />
    <ProjectReference Include="..\UET.EGarden.Core\UET.EGarden.Core.csproj" />
    <ProjectReference Include="..\UET.EGarden.EntityFrameworkCore\UET.EGarden.EntityFrameworkCore.csproj" />
  </ItemGroup>
